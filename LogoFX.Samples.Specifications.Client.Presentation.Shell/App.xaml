<Application x:Class="LogoFX.Samples.Specifications.Client.Presentation.Shell.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:shell="clr-namespace:LogoFX.Samples.Specifications.Client.Presentation.Shell"
             xmlns:xaml="clr-namespace:LogoFX.Client.Mvvm.View.Infra.Xaml;assembly=LogoFX.Client.Mvvm.View.Infra">
    <Application.Resources>
        <xaml:SharedResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <xaml:SharedResourceDictionary>
                    <shell:AppBootstrapper x:Key="AppBootstrapper" />
                 </xaml:SharedResourceDictionary>
             </ResourceDictionary.MergedDictionaries>
         </xaml:SharedResourceDictionary>
    </Application.Resources>
</Application>
